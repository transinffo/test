	function inZone(el) {
		if($(el).length) {
			var scrollTop = $(window).scrollTop();
			var windowHeight = $(window).height();
			var offset = $(el).offset();
			if(scrollTop <= offset.top && ($(el).height() + offset.top) < (scrollTop + windowHeight)) return true;
		};
		return false;
	}
